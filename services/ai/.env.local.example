# AI Service Local Development Configuration
# Copy this file to .env.local and fill in your values

# OpenRouter Configuration
OPENROUTER_API_KEY=your-openrouter-api-key-here
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
OPENROUTER_SITE_URL=https://salonflow.app
OPENROUTER_SITE_NAME=Salon Flow

# Model Configuration
DEFAULT_MODEL=google/gemini-2.5-flash
FALLBACK_MODEL=google/gemini-2.0-flash-001
MAX_TOKENS=4096
TEMPERATURE=0.7

# Upstash Redis (get from GCP Secret Manager or Upstash dashboard)
UPSTASH_REDIS_REST_URL=your-upstash-redis-url
UPSTASH_REDIS_REST_TOKEN=your-upstash-redis-token
CACHE_TTL=3600

# API Service - Connect to LIVE backend
API_SERVICE_URL=https://salon-flow-api-687369167038.asia-south1.run.app

# Feature Flags
ENABLE_CACHE=true
ENABLE_LOGGING=true
DEBUG=true
ENVIRONMENT=development
